{
    "name": "vandetho/symflow-bundle",
    "type": "symfony-bundle",
    "version": "1.0.0",
    "require": {
        "php": ">=8.1",
        "symfony/validator": "^6.4 || ^7.0",
        "symfony/http-kernel": "^6.4 || ^7.0",
        "symfony/config": "^6.4 || ^7.0",
        "symfony/dependency-injection": "^6.4 || ^7.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^6.4 || ^7.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Symflow\\SymflowBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symflow\\SymflowBundle\\Tests\\": "tests/"
        }
    }
}